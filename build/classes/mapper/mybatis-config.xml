<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<plugins>
		<plugin interceptor="com.enation.framework.pager.simple.PagePlugin">
 			<property name="dialect" value="mysql"/>    
	        <property name="pageSqlId" value=".*ByPage.*"/> 		
		</plugin>
	</plugins>	
	<mappers>
		 <mapper  resource="mapper/glodmine/IncomeMapper.xml"/>
		 <mapper  resource="mapper/glodmine/IncomeTypeMapper.xml"/>
		 <mapper  resource="mapper/glodmine/MemberMapper.xml"/>
		 <mapper  resource="mapper/glodmine/SpeedMapper.xml"/>
		 <mapper  resource="mapper/glodmine/SpeedTypeMapper.xml"/>
	</mappers>
	

</configuration>
